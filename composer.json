{
    "name": "mmucklo/grid-bundle",
    "description": "Datagrid for symfony2 or symfony3 or symfony4",
    "keywords": ["grid", "jQGrid", "table"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Tee"
        },
        {
            "name": "Matthew J Mucklo",
            "email": "mmucklo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "2.*|3.*|4.*",
        "sensio/framework-extra-bundle": "2.*|3.*|4.*|5.*",
        "symfony/templating": "2.*|3.*|4.*",
        "symfony/twig-bundle": "2.*|3.*|4.*",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "doctrine/annotations": "<1.5",
        "doctrine/cache": "<1.7",
        "doctrine/collections": "<1.5",
        "doctrine/instantiator": "<1.1",
        "doctrine/common": "<2.8",
        "doctrine/dbal": "<2.6",
        "doctrine/mongodb-odm": "^1.1",
        "phpunit/phpunit": "^5.7.0",
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpunit/php-code-coverage": "^4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Dtc\\GridBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
